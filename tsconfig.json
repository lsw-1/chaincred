{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noEmit": true,
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "@utils/*": [
        "lib/utils/*"
      ],
      "@components/*": [
        "lib/components/*"
      ]
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
  ]
}